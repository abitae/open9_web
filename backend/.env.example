# MySQL o MariaDB DSN. Usa 127.0.0.1 en lugar de localhost si ves error "auth_gssapi_client"
DATABASE_URL=root:root@tcp(127.0.0.1:3306)/open9?parseTime=true

# Secret to sign JWT tokens (use a strong value in production)
JWT_SECRET=your-secret-key-change-in-production

# Optional: default admin password when seeding the first user (username: admin)
ADMIN_DEFAULT_PASSWORD=admin123

# Optional: server port (default 8080)
PORT=8080
